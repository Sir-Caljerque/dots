[{
  "position": "top",
  "name": "systemBar",
  "output": "!HDMI-A-1",
  "height": 25,
  "spacing": 10,
  "reload_style_on_change": true,
  "modules-left": [
    "keyboard-state",
    "tray",
    "privacy"
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "wireplumber",
    "network",
    "cpu",
    "memory",
    "temperature",
    "backlight",
    "clock",
    "group/group-power"
  ],
  "hyprland/workspaces": {
    "active-only": false,
    "all-outputs": false,
    "format": "{name}", // <sub>{icon}</sub> {windows}",
    "format-icons": {
      "active": "ïˆ…",
      "default": "ïˆ„",
      "empty": "âˆ…",
      "persistent": "âˆ…",
      "urgent": "ïª"
    },
    "persistent-workspaces": {
      "*": 5
    },
    "show-special": true,
    "special-visible-only": true,
    "sort-by": "number",
    "window-rewrite": {
      "title<.*youtube.*>": "ï…ª",
      "class<pavucontrol> title<Volume Control>": "ó°•¾",
      "title<Easy Effects>": "ó°•¾",
      "class<firefox>": "ï‰©",
      "class<kitty>": "î•",
      "class<webcord>": "ó°™¯",
      "class<obsidian>": "ğŸª¨",
      "class<steam>": "ï†¶",
      "class<heroic>": "ó°’™",
      "class<kitty> title<nv.*>": "ó°¡",
      "class<kitty> title<.*.conf>": "î˜•",
      "class<kitty> title<.*.c> && !title<.*.jsonc>": "î˜",
      "class<kitty> title<.*.zig>": "îš©",
      "class<kitty> title<.*.rs>": "îš‹",
      "class<kitty> title<.*.py>": "î˜†",
      "class<kitty> title<.*.sh>": "î¯Š",
      "class<kitty> title<.*.js>": "î˜Œ",
      "class<kitty> title<.*.jsonc>": "ó°˜¦",
      "class<kitty> title<.*.json>": "î˜‹",
      "class<kitty> title<.*vim.*>": "îŸ…"
    },
    "window-rewrite-default": "î¯†",
    "format-window-separator": "",
    "move-to-monitor": false
  },
  "hyprland/window": {
    "format": "", // "[{class}] - {title}",
    "separate-outputs": true,
    "icon": true,
    "icon-size": 20
  },
  "hyprland/language": {
    "format": "Lang: {}",
    "format-en": "AMERICA, HELL YEAH!"
  },
  "hyprland/submap": {
    "format": "sub: {}",
    // "max-length": ,
    // "on-click": ,
    // "on-click-middle": ,
    // "on-click-right": ,
    // "on-scroll-up": ,
    // "on-scroll-down": ,
    // "rotate": ,
    // "smooth-scrolling-threshold": ,
    "tooltip": true,
    "always-on": true,
    "default-submap": "Default"
  },
  "keyboard-state": {
    "numlock": true,
    "capslock": true,
    "format": "{name}",
    "format-icons": {
      "locked": "ï€£",
      "unlocked": "ï‚œ"
    },
    "binding-keys": [58, 69, 70]
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ï®",
      "deactivated": "ï°"
    }
  },
  "tray": {
    "icon-size": 15,
    "show-passive-items": false,
    "spacing": 5
  },
  "clock": {
    "interval": 60,
    "format": "{:%H:%M â˜€}",
    "tooltip": true,
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
    "format-alt": "{:%m/%d ï„³}",
    "calendar": {
      "mode": "month",
      "weeks-pos": "left",
      "format": {
        "weeks": "{:%U|}",
        "today": "<b><u><i>{}</i></u></b>"
      }
    },
    "actions": {
      "on-click-right": "mode"
    }
  },
  "cpu": {
    "interval": 1,
    "format": "{usage}%{icon0}{icon1}{icon2}{icon3}{icon4}{icon5}{icon6}{icon7}",
    "format-icons": [
      "<span color='#69ff94'>â–</span>",
      "<span color='#2aa9ff'>â–‚</span>",
      "<span color='#f8f8f2'>â–ƒ</span>",
      "<span color='#f8f8f2'>â–„</span>",
      "<span color='#ffffa5'>â–…</span>",
      "<span color='#ffffa5'>â–†</span>",
      "<span color='#ff9977'>â–‡</span>",
      "<span color='#dd532e'>â–ˆ</span>"
    ],
    "tooltip": true
  },
  "memory": {
    "interval": 10,
    "format": "{used:0.1f}GiB ó°›",
    "tooltip": true,
    "tooltip-format": "{swapUsed:0.1f}GiB swap"
  },
  "temperature": {
    "critical-threshold": 80,
    "format": "{temperatureC}Â°C {icon}",
    "format-icons": [
      "ï«",
      "ï‹‰",
      "ï©"
    ]
  },
  "backlight": {
    "format": "{percent}% {icon}",
    "format-icons": [
      "î",
      "î“",
      "î‘",
      "î",
      "î",
      "î",
      "îŠ",
      "îˆ",
      "î›"
    ]
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-full": "{capacity}% {icon}",
    "format-charging": "{capacity}% ï—§",
    "format-plugged": "{capacity}% ï‡¦",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "ï‰„",
      "ï‰ƒ",
      "ï‰‚",
      "ï‰",
      "ï‰€"
    ]
  },
  "battery#bat2": {
    "bat": "BAT2"
  },
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "default": "ïƒ§",
      "performance": "ïƒ§",
      "balanced": "ï‰",
      "power-saver": "ï¬"
    }
  },
  "network": {
    "format-wifi": "{essid} ({signalStrength}%) ï‡«",
    "format-ethernet": "{ipaddr}/{cidr} ï–",
    "tooltip-format": "{ifname} via {gwaddr} ï–",
    "format-linked": "{ifname} (No IP) ï–",
    "format-disconnected": "Disconnected âš ",
    "format-alt": "{ifname}: {ipaddr}/{cidr}"
  },
  "wireplumber": {
    "format": "{volume}% {icon}",
    "format-muted": "ï€¦",
    "format-icons": [
      "ï€¦",
      "ï€§",
      "ï€¨"
    ],
    "on-click": "pavucontrol",
    "on-click-right": "easyeffects",
    "max-volume": 150,
    "scroll-step": 5
  },
  "privacy": {
    "icon-spacing": 4,
    "icon-size": 18,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 20
      },
      {
        "type": "audio-out",
        "tooltip": true,
        "tooltip-icon-size": 20
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 20
      }
    ]
  },
  "group/group-power": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "children-class": "not-power",
      "transition-left-to-right": false
    },
    "modules": [
      "custom/power", // First element is the "group leader" and won't ever be hidden
      "custom/quit",
      "custom/lock",
      "custom/reboot",
      "custom/hibernate"
    ]
  },
  "custom/quit": {
    "format": "ó°—¼",
    "tooltip": false,
    "on-click": "hyprctl dispatch exit"
  },
  "custom/lock": {
    "format": "ó°",
    "tooltip": false,
    "on-click": "hyprlock"
  },
  "custom/reboot": {
    "format": "ó°œ‰",
    "tooltip": false,
    "on-click": "reboot"
  },
  "custom/power": {
    "format": "ï€‘",
    "tooltip": false,
    "on-click": "shutdown now"
  },
  "custom/hibernate": {
    "format": "ó°œ—",
    "tooltip": false,
    "on-click": "systemctl hibernate"
  }
},
{
  "position": "bottom",
  "name": "workspacesBar",
  "output": "!HDMI-A-1",
  "height": 25,
  "spacing": 0,
  "reload_style_on_change": true,
  "modules-left": [],
  "modules-center": [
    "hyprland/workspaces"
  ],
  "modules-right": [],
  "hyprland/workspaces": {
    "active-only": false,
    "all-outputs": false,
    "format": "", // <sub>{icon}</sub> {windows}",
    "format-icons": {},
    "persistent-workspaces": {
      "*": 5
    },
    "show-special": true,
    "special-visible-only": true,
    "sort-by": "number",
    "window-rewrite": {},
    "window-rewrite-default": "",
    "format-window-separator": "",
    "move-to-monitor": false
  }
}]
